<div class="container">
    <h1>Tela de Consulta</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Endereço</th>
                <th>Cidade</th>
                <th>Telefone</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
            {{#each posts}}
            {{#dataValues}}
            <tr>
                <td>{{nome}}</td>
                <td>{{endereco}}</td>
                <td>{{cidade}}</td>
                <td>{{telefone}}</td>
                <td>
                    <button class="btn btn-primary"><a class="btn btn-sm" href="/editar/{{id}}"
                            >Editar</a></button>
                    <button class="btn btn-danger"><a class="btn btn-sm" href="/excluir/{{id}}"
                            >Excluir</a></button>
                </td>
            </tr>
            {{/dataValues}}
            {{/each}}
        </tbody>
    </table>
</div>